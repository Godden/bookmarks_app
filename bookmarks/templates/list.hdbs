<div data-sheet-name="bookmarks">
  <ul>
    {{#bookmarks}}<li>
      <a href="#/tickets/{{ticket.nice_id}}">{{ticket.subject}}</a>
    </li>{{/bookmarks}}
  </ul>
  <p>{{^bookmarks}}{{t "none"}}{{/bookmarks}}</p>
  <button class="btn btn-inverse bookmark">{{t "bookmark_this_ticket"}}</button>
</div>
