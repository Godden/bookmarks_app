<div data-sheet-name="bookmarks">
  <h3>{{t "title"}}</h3><hr/>
  <ul>
    {{#bookmarks}}<li>
      <a href="#/tickets/{{ticket.nice_id}}">{{ticket.subject}}</a>
    </li>{{/bookmarks}}
  </ul>
  {{^bookmarks}}{{t "none"}}{{/bookmarks}}
  <button class="btn bookmark">{{t "bookmark_this_ticket"}}</button>
</div>
